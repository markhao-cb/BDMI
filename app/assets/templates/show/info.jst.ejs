<h3 class="section-title">Movie Details</h3>
<div class="movie-list detail-item">
  <ul id="movie-info-list">
    <li class="movie-info-item">
      <h4 class="info-title">Title :</h4>
      <h3 class="info-subitem"><%= movie.escape("title") %></h3>
    </li>
    <li class="movie-info-item">
      <h4 class="info-title">Year :</h4>
      <h3 class="info-subitem"><%= movie.escape("yr") %></h3>
    </li>
    <li class="movie-info-item">
      <h4 class="info-title">Score :</h4>
      <div class="small-movie-star info-subitem">
    </li>
    <li class="movie-info-item">
      <h4 class="info-title">Votes :</h4>
      <h3 class="info-subitem"><%= movie.escape("votes") %></h3>
    </li>
  </ul>
</div>
<div class="movie-poster detail-item">
</div>

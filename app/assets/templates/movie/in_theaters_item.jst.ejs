<div id="movie_image">
  <img src="<%= movie.escape("img_url") %>" class="small_image"></img>
</div>
<div id="movie_title">
  <a href="#/movies/<%=movie.get("id")%>" class="small_title">
    <%= movie.escape("title") %>
  </a>
  <span id="movie_score">
    <%= movie.escape("score") %>
  </span>
</div>

